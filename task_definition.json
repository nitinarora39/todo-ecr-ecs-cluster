{
  "family": "to-do-app",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "to-do-app",
      "image": "226127612063.dkr.ecr.us-east-1.amazonaws.com/jenkins/to-do-app:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 80
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::226127612063:role/ecsTaskExecutionRole"
}
